      "    zrt_ids,",
      "    expiration_date,",
      "    toestandsdatum",
      "  FROM brk_prepared.stukdeel",
      ") sdl"
    ],
    "recent": [
      ", (",
      "    SELECT",
      "      MIN(last_id) AS last_id",
      "    FROM brk_metadata.last_source_id",
      "    WHERE collection = 'stukdeel' AND",
      "    date_registered > NOW() - INTERVAL '10 days'",
      "  ) l",
      "WHERE sdl.nrn_sdl_id > l.last_id"
