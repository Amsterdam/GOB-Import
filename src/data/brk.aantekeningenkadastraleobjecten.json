      "    brk_sjt_ids,",
      "    nrn_sdl_ids,",
      "    toestandsdatum,",
      "    expiration_date,",
      "    begindatum",
      "  FROM brk_prepared.aantekening_kadastraal_object",
      ") atg"
    ],
    "recent": [
      ", (",
      "    SELECT",
      "      MIN(last_id) AS last_id",
      "    FROM brk_metadata.last_source_id",
      "    WHERE collection = 'aantekening_kadastraal_object' AND",
      "    date_registered > NOW() - INTERVAL '10 days'",
      "  ) l",
      "WHERE atg.nrn_atg_id > l.last_id"
    ]
  },
